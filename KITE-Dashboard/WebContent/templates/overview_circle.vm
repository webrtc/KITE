<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>Overview - KITE Dashboard</title>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/pace.min.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/themes/black/pace-theme-minimal.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="assets/css/light-bootstrap-dashboard.css" rel="stylesheet"/>
    <link href="assets/css/kite-dashboard.css" rel="stylesheet"/>

    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="https://rawgithub.com/olimsaidov/pixeden-stroke-7-icon/master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css" rel="stylesheet" />

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.js" type="text/javascript"></script>
    <script src="https://d3js.org/d3.v3.min.js"></script>
    <script src="assets/js/sunburstv2.js"></script>
    <script src="assets/js/overview.js"></script>
    <script>
        var width = $( window ).width()/2.8;
        var browser = 'all';
        var testName = '$testname';
        var tupleSize = $tupleSize;
        var classes = ['btn btn-success', 'btn btn-danger', 'btn btn-info', 'btn btn-warning'];
        var display = [];
        display[0] = true;
        display[1] = true;
        display[2] = true;
        display[3] = false;
    </script>
    <style>
        text{
          font-family: Arial, sans-serif;
          font-size: 10px;
        }
    </style>
</head>
<body>
    <div class="wrapper">
        <!-- Start of side nav bar -->
        <div class="sidebar" data-color="black" data-image="assets/img/sidebarSmall.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="dashboard" class="simple-text">
                        KITE
                    </a>
                </div>

                <ul class="nav">
                    <li>
                        <div class="row">
                            <div class="col-sm-1 col-sm-offset-1">
                                <i class="pe-7s-search"></i>
                            </div>
                            <div class="col-sm-9 ">
                                <form action="search" method="get">
                                  <div>
                                    <input name="key" id="key" placeholder="Search">
                                  </div>
                                </form>
                            </div>

                        </div>
                    </li>
                    <li>
                        <a href="dashboard"> <!-- change link to dashboard -->
                            <i class="pe-7s-note2"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="score">
                            <i class="pe-7s-keypad"></i>
                            <p>Scoreboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="versions">
                            <i class="pe-7s-way"></i>
                            <p>Version Info</p>
                        </a>
                    </li>
                    <li class="active">
                        <nav class="overview">
                            <ul>
                                <li><a href="#">
                                        <i class="pe-7s-pin"></i>
                                        <p>Overview</p>
                                    </a>
                                    <ul>
                                        #foreach($test in $listOfTest)
                                        <li><a href="overview?testname=$linkTool.encode($test.getTestName())&size=$linkTool.encode($test.getTupleSize())" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #end
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End of side nav bar -->
        <!-- Start main data content display -->
        <div class="main-panel">
        <!-- Start horizontal nav bar -->
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#">
                            <i class="pe-7s-menu"></i>
                        </button>
                        <a class="navbar-brand" href="dashboard">Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                    </div>
                </div>
            </nav>
        <!-- End of horizontal nav bar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-4 col-sm-12" style="margin-right:70px">
                            <div class="card">
                                <div class="header">
                                    <h5>*Showing results for browsers with latest versions stable and canary (nightly) only</h5>
                                    <div class="dropdown">
                                        <h5>Filter:
                                        <button id="0" type="button" class="btn btn-success"><i class="pe-7s-check"></i></button><button id="1" type="button" class="btn btn-danger"><i class="pe-7s-close-circle"></i></button>
                                        <button id="2" type="button" class="btn btn-info"><i class="pe-7s-attention"></i></button><button id="3" type="button" class="btn btn-warning"><i class="pe-7s-timer"></i></button></h5>
                                        <h5 class="filter-by-browser dropdown-toggle" data-toggle="dropdown" id="filter-by-browser">All browsers</h5>
                                        <ul class="dropdown-menu result-wrapping">
                                            <li class="by-browser" id="all"><a >All browsers</a></li>
                                            #foreach($browser in $browserList)
                                                <li class="by-browser" id="$browser.getName()-$browser.getVersion()-$browser.getPlatform()"><a>
                                                #if ($browser.getName()=="safari")
                                                    <img src="assets/img/safari.png" height="20" width="20">
                                                #elseif ($browser.getName()=="firefox")
                                                    <img src="assets/img/firefox.png" height="20" width="20">
                                                #elseif ($browser.getName()=="chrome")
                                                    <img src="assets/img/chrome.png" height="20" width="20">
                                                #elseif ($browser.getName()=="MicrosoftEdge")
                                                    <img src="assets/img/edge.png" height="20" width="20">
                                                #end
                                                $browser.getVersion() - $browser.getPlatform()</a></li>
                                            #end
                                        </ul>
                                    </div>
                                    <hr>
                                </div>
                            </div>
                            <div class="card">
                                <div class="header">
                                    <h4 class="title">Overview: $testname</h4>
                                    <h5 class="small-boy">$description</h5>
                                    <p><p>
                                </div>
                                <div class="content">
                                <!-- start content -->
                                    <table class="table">
                                        <thead>
                                            <th>Total</th>
                                            <th>Successful</th>
                                            <th>Failed</th>
                                            <th>Error</th>
                                        </thead>
                                        <tbody>
                                            <tr class="status">
                                                <td id="total">$stats.get(0)</td>
                                                <td id="successful">$stats.get(1) ($stats.get(5) %)</td>
                                                <td id="failed">$stats.get(2) ($stats.get(6) %)</td>
                                                <td id="error">$stats.get(3) ($stats.get(7) %)</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <div class="wrapping">
                                        <div class="content table-responsive run-wrapping">
                                            <table class="table table-hover">
                                                <tbody>
                                                <a>Last run: $dateTool.format($lastRun)</a>
                                                <hr>
                                                <h5>$listOfTests.size() run(s):</h5>
                                                    #set($index = 1)
                                                    #foreach($test in $listOfTests)
                                                    <tr>
                                                        <td style="border:none"><a class="small-boy" href="results?test=$linkTool.encode($test.getTestId())">$index. $dateTool.format($test.getStartTime()) </a></td>
                                                        <td style="border:none"><a class="small-boy" href="results?test=$linkTool.encode($test.getTestId())">$test.getTotalTests() test case(s) </a></td>
                                                    </tr>
                                                    #set($index = $index + 1)
                                                    #end
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="overviewCard" class="card col-md-7 col-sm-12">
                            <div id="oldLayer"></div>
                            <div id="sequence" style="margin-right:70px"></div>
                            <!-- start content -->
                            <div class="content row" style="height:40vw">
                                <div class="col-md-2 col-sm-2">
                                    <div id="legend"></div>
                                </div>
                                <div class="col-md-10 col-sm-10">
                                    <div id="chart"></div>
                                </div>
                                <script>
                                    myData=$jsonData;
                                    drawCircle(myData.sunburst);
                                </script>
                            </div>
                            <!-- end content -->
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of main data content display -->
        </div>
    </div>
</body>
    <!--   Core JS Files   -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="assets/js/light-bootstrap-dashboard.js"></script>
</html>