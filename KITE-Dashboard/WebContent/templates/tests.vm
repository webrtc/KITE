<html lang="en">
<head>

    <meta charset="utf-8" />
    <title>Tests - KITE Dashboard: $configName</title>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/themes/black/pace-theme-minimal.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />
    <link href="assets/css/light-bootstrap-dashboard.css" rel="stylesheet"/>
    <link href="assets/css/kite-dashboard.css" rel="stylesheet"/>
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="https://rawgithub.com/olimsaidov/pixeden-stroke-7-icon/master/pe-icon-7-stroke/dist/pe-icon-7-stroke.css" rel="stylesheet" />

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.0/pace.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.4/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/progressbar.js/1.0.1/progressbar.min.js"></script>
    <script src="assets/js/test.js"></script>
    <script>
        var done = 0;
        var chartMap = new Map();
    </script>

</head>
<body>
    <div class="wrapper">
        <!-- Start of side nav bar -->
        <div class="sidebar" data-color="black" data-image="assets/img/sidebarSmall.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="dashboard" class="simple-text">
                        KITE
                    </a>
                </div>
                <ul class="nav">
                    <li>
                        <div class="row">
                            <div class="col-sm-1 col-sm-offset-1">
                                <i class="pe-7s-search"></i>
                            </div>
                            <div class="col-sm-9 ">
                                <form action="search" method="get">
                                  <div>
                                    <input name="key" id="key" placeholder="Search">
                                  </div>
                                </form>
                            </div>

                        </div>
                    </li>
                    <li class="active">
                        <a href="dashboard"> <!-- change link to dashboard -->
                            <i class="pe-7s-note2"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="score">
                            <i class="pe-7s-keypad"></i>
                            <p>Scoreboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="versions">
                            <i class="pe-7s-way"></i>
                            <p>Version Info</p>
                        </a>
                    </li>
                    <li>
                        <nav class="overview">
                            <ul>
                                <li><a href="#">
                                        <i class="pe-7s-pin"></i>
                                        <p>Overview</p>
                                    </a>
                                    <ul class="result-wrapping">
                                        #foreach($test in $listOfTest)
                                        #if($test.getTupleSize()==2)
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=true" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #else
                                          <li><a href="overview?testname=$linkTool.encode($test.getTestName())&1v1=false" style="font-size: 12px;" >$test.getTestName()<div class="circle"></div></a></li>
                                        #end
                                        #end
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End of side nav bar -->
        <!-- Start main data content display -->
        <div class="main-panel">
        <!-- Start horizontal nav bar -->
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#">
                            <i class="pe-7s-menu"></i>
                        </button>
                        <a class="navbar-brand" href="dashboard">Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                    </div>
                </div>
            </nav>
        <!-- End of horizontal nav bar -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="header">
                                    <h3 class="title">$configName </h3>
                                    <h5>Started at: $dateTool.format($listOfTest.get(0).getStartTime()) - Including $listOfTest.size() test(s) </h5>
                                    </br>
                                </div>
                            </div>
                        </div>
                        #set($index = 1)
                        #set($marge = $dateTool.getSystemTime() - $listOfTest.get(0).getStartTime())
                        #foreach($test in $listOfTest)
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="card">
                                <div class="header">
                                    <a class="medium-boy" href="results?test=$linkTool.encode($test.getTestId())">$index. $test.getTestName()</a>
                                    <h5 class="small-boy">$test.getDescription()</h5>
                                </div>
                                <div class="content">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4 col-sm-4">
                                            <div>
                                                <canvas id="$test.getTestName()"></canvas>
                                            </div>
                                        <script>
                                            var id = $test.getTestId();
                                            initialSetup(id);
                                        </script>
                                        </div>
                                        <div class="col-lg-8 col-md-8 col-sm-8">
                                            <div class="row">
                                                <div class="col-lg-4 col-md-4 col-sm-4">
                                                    <h5 class="small-boy">Implementation Class</h5>
                                                    <h5 class="small-boy">Tuple size</h5>
                                                    <h5 class="small-boy">Number of test cases</h5>
                                                    <h5 class="small-boy">Status</h5>
                                                    <h5 class="small-boy">ETA</h5>
                                                </div>
                                                <div class="col-lg-8 col-md-8 col-sm-8">
                                                    <h5 class="small-boy">: $test.getImpl()</h5>
                                                    <h5 class="small-boy">: $test.getTupleSize()</h5>
                                                    <h5 class="small-boy">: $test.getTotalTests()</h5>
                                                #if($test.getStatus())
                                                    <h5 class="ended">: Done</h5>
                                                    <h5 class="ended">: Ended at $dateTool.format($test.getEndTime())</h5>
                                                #elseif ($marge > 86400000)
                                                    <h5 class="interrupted">: Interrupted</h5>
                                                    <h5 class="interrupted">: NC</h5>
                                                #else
                                                    <div class="small-boy">
                                                        <i class="fa fa-spinner fa-spin" style="font-size:24px"></i>
                                                    </div>
                                                    <h5 class="small-boy" id="progress$index"></h5>
                                                    <h5 class="small-boy" id="endTime$index"></h5>
                                                    <script>
                                                        var total = $test.getTotalTests();
                                                        var ETA = '$dateTool.getDate()';
                                                        var id = $test.getTestId();
                                                        var index = $index;
                                                        var tmp;
                                                        getProgress(id, index);
                                                    </script>
                                                #end
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        #set($index = $index + 1)
                        #end
                    </div>
                </div>
            </div>
            <!-- End of main data content display -->
        </div>
    </div>
</body>
    <!--   Core JS Files   -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="assets/js/light-bootstrap-dashboard.js"></script>
</html>